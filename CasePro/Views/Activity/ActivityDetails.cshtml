                    new SelectListItem { Text = "Barrier 1", Value = "Barrier1" }, // Default option
                    new SelectListItem { Text = "Barrier 2", Value = "Barrier2" },
                    new SelectListItem { Text = "Barrier 3", Value = "Barrier3" }
                    },
                    new { @class = "form-control", id = "btype" }
                    )

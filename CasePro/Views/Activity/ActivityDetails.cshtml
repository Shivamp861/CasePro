                        @Html.TextBoxFor(x => x.SupplierContact, new { @class = "form-control", id="sitesupplierContact"})
                        <span id="sitesupplierError" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-sm-6 col-md-3">
                    <div class="form-group clearfix">
                        <label for="Numberofpersons">Number or persons supplied:</label>
                        @Html.TextBoxFor(x => x.NoOfPersoneSupplied, new { @class = "form-control", id="Numberofpersons"})

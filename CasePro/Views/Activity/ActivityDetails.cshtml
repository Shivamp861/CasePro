                    new SelectListItem { Text = "Yes", Value = "Yes" }, // Default option
                    new SelectListItem { Text = "No", Value = "No" }
                    },
                    new { @class = "form-control", id = "bpermitreqchain" }
                    )
                        <span id="permitreqchainError" class="text-danger"></span>
                    </div>


                </div>
                <div class="col-sm-12 col-md-6">
                    <div class="form-group clearfix">
                        <label for="Incidentreporting">Incident reporting:</label>
                        @Html.TextBoxFor(x => x.IncidentReporting, new { @class = "form-control", id="Incidentreporting"})
                        <span id="IncidentreportingError" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <!--end row3-->
            <!--row4-->
            <div class="row">
                <div class="col-sm-12 col-md-6">
                    <div class="form-group clearfix">
                        <label for="blocations">Other resources / equipment used:</label>
                        @Html.TextAreaFor(model => model.OtherResourcesEquipmentUsed, new { @class = "form-control", id = "txtresources", cols = "5", rows = "5" })
                        <span id="txtresourcesError" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-sm-12 col-md-6">
                    <div class="form-group clearfix">
                        <label for="special">
                            Any special instructions / site restrictions / specific PPE
                            :
                        </label>
                        @Html.TextAreaFor(model => model.AnySpecialInstructions, new { @class = "form-control", id = "txtspecialinstruction", cols = "5", rows = "5" })

                        <span id="txtspecialinstructionError" class="text-danger"></span>
                    </div>
                </div>

            </div>
            <div class="row">
                <div class="col-sm-24 col-md-12">
                    <div class="form-group clearfix">
                        <label for="blocations">All relevant activity RAMS, lift plans and SSOW:</label>
                        @Html.TextAreaFor(model => model.AllRelevantActivityRams, new { @class = "form-control", id = "txtrelevantactivity", cols = "5", rows = "5" })
                        <span id="txtrelevantactivityError" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <!--end row4-->
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group clearfix">
                        @Html.Label("permitreq", "Site/Layout drawings/ site images :")
                        <div class="our_img">
                        @if (Model.activityImages.Count > 0)
                        {
                            for (int i = 0; i < Model.activityImages.Count; i++)
                            {
                                <div class="images_blog1">
                                    <img src="@Model.activityImages[i].UploadPath" alt="Display" id="UploadPath" style="max-width: 100px; max-height: 100px;" />
                                    @Html.HiddenFor(model => model.activityImages[i].Imgid)
                                    @Html.HiddenFor(model => model.activityImages[i].UploadPath)
                                </div>
                            }
                        }
                        </div>
                        @Html.TextBoxFor(Model => Model.UploadPath, new { @class = "form-control-file Installation", type = "file", id = "fileInput",name = "SiteImages[]", @multiple = "multiple",onchange="displayThumbnails(event)"})
                        <span id="fileInputError" class="text-danger fileError"></span>
                        @Html.HiddenFor(model => model.Imgid, new { @class = "form-control", id="Imgid"})
                    </div>
                </div>
                </div>
                <div class="row">
                <div class="col-sm-12 col-md-12">
                    <div class="our_thumbnails_img" id="thumbnails"></div>
                </div>
            </div>
        </div>
    </div>
}

                    </div>
                    <span id="btypeerror " class="text-danger"></span>

                </div>
                <div class="col-sm-6 col-md-3">
                    <div class="form-group clearfix">
                        <label for="quantity">Quantity of installed Barriers:</label>
                        @Html.TextBoxFor(x => x.BarrierQty, new { @class = "form-control", id="bquantity"})
                        <span id="qtyBarriersError" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-sm-12 col-md-6">
                    <div class="form-group clearfix">
                        <label for="blocations">Barrier start and finish location:</label>
                        @Html.TextBoxFor(x => x.BarrierStartAndFinishLocation, new { @class = "form-control", id="blocations"})
                        <span id="blocationError" class="text-danger"></span>
                    </div>

                </div>
            </div>

            <div class="row">
                <div class="col-sm-6 col-md-3">
                    <div class="form-group clearfix">
                        <label for="bPerformance">Barrier Performance :</label>
                        @Html.TextBoxFor(x => x.BarrierPerformance, new { @class = "form-control", id="bPerformance"})
                        <span id="bperformanceError" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-sm-6 col-md-3">
                    <div class="form-group clearfix">

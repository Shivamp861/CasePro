<style>
    .donepopup {
        border: 1px solid #00a65a;
        padding: 15px 20px;
        position: absolute;
        top: 0;
        right: 0;
        background: #00a65a;
        color: #fff;
    }


    .errorpopup {
        border: 1px solid red;
        padding: 15px 20px;
        position: absolute;
        top: 0;
        right: 0;
        background: red;
        color: #fff;
    }

    .panel-heading p {
        margin: 0;
    }


    .panel-default > .panel-heading {
        position: relative;
    }

    .our_img {
        display: inline-block;
        grid-gap: 20px;
        align-items: center;
        flex-wrap: wrap;
        column-count: 12;
    }



    .images_blog1 img {
        width: 100%;
        height: 100%;
    }

    .Installation {
        margin-top: 20px;
    }

    .loading{
        margin-top: 20px;
    }

    .Tipping {
        margin-top: 20px;
    
    }

    .images_blog1 {
        height: 100px;
        width: 100px;
        margin-bottom: 30px;
    }

    .our_thumbnails_img {
        display: inline-flex;
       
        object-fit: cover;
    }

        .our_thumbnails_img img {
            object-fit: cover;
        }

    .thumbnail {
        height: 70px;
    }

    .our_thumbnails_img {
        display: inline-flex;
        object-fit: cover;
        flex-wrap: wrap;
        column-count: 8;
    }

</style>
@if (Model.Type == "Site Installation" || Model.Type == "Site Uplift")
{

    <div class="panel panel-default siteInstallation">
        <div class="panel-heading">
            @if (Model.Type == "Site Installation")
            {
                <h4> Site Installation</h4>
            }
            else
            {
                <h4> Site Uplift</h4>
            }

            <div id="sitesuccessmessage" class="donepopup" style="display:none">
                <p>Site Installation successfully saved</p>
            </div>
            <div id="sitesuccessupdatemessage" class="donepopup" style="display:none">
                <p>Site Installation successfully Updated</p>
            </div>
            <div id="siteerrormessage" class="errorpopup" style="display:none">
                <p>Error while saving data </p>
            </div>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-sm-6 col-md-3">
                    <div class="form-group clearfix">
                        <input type="hidden" id="Id" class="sid1" name="Id" value="@Model.Id" />
                        @Html.HiddenFor(model => model.ActivityId, new { @class = "form-control", id="activityid"})
                        <label for="sitemeetingDate">Meeting site:</label>
                        @Html.TextBoxFor(x => x.MeetingSite, new { @class = "form-control", id="sitemeetingDate"})
                        <span id="sitemeetingError" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-sm-6 col-md-3">
                    <div class="form-group clearfix">
                        <label for="sitelabourSupplier">Labour Supplier:</label>
                        @Html.TextBoxFor(x => x.LabourSupplier, new { @class = "form-control", id="sitelabourSupplier"})
